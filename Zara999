-- Welcome Text
local welcomeText = Instance.new("TextLabel")
welcomeText.Size = UDim2.new(1, 0, 0, 20)
welcomeText.Position = UDim2.new(0, 0, 1, 0)
welcomeText.BackgroundTransparency = 1
welcomeText.Text = "Welcome !!!"
